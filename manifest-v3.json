{
  "manifest_version": 3,
  "name": "YApi2Code",
  "version": "0.0.1",
  "description": "Turning YApi docs into Admate codes",
  "author": "cloydlau",
  "action": {
    "default_title": "YApi2Code",
    "default_popup": "dist/index.html"
  },
  "icons": {
    "16": "public/favicon.png",
    "48": "public/favicon.png",
    "128": "public/favicon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "omnibox": {
    "keyword": "yapi2code"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
}
